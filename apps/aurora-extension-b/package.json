    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "tsx": "^4.19.2",
    "zod": "^3.24.1"
