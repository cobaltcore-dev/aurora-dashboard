{
  "name": "@cobaltcore-dev/aurora-portal",
  "private": true,
  "version": "1.0.0",
  "main": "dist/client/index.js",
  "scripts": {
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --mode=production",
    "build:server": "tsc -p tsconfig.server.json",
    "check-i18n": "lingui compile --strict",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "clean:cache": "rm -rf .turbo",
    "clean:extensions": "tsx scripts/remove-extensions.ts",
    "compile": "lingui compile --typescript",
    "dev": "tsx watch src/server/server --tsconfig tsconfig.server.json",
    "extract": "lingui extract --clean",
    "lint": "eslint",
    "postinstall": "tsx --no-cache scripts/extensions/install.ts && rm -rf tsx-0",
    "preview": "vite preview",
    "prod": "NODE_ENV=production node dist/server/server.js",
    "test": "vitest run",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cloudoperators/juno-k8s-client": "^1.0.5",
    "@cobaltcore-dev/aurora-sdk": "workspace:*",
    "@cobaltcore-dev/signal-openstack": "workspace:*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.0.3",
    "@headlessui/react": "^2.2.2",
    "@lingui/core": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@material-design-icons/svg": "^0.14.15",
    "@tanstack/react-router": "^1.115.2",
    "@trpc/client": "11.0.0-rc.730",
    "@trpc/server": "11.0.0-rc.730",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.3.3",
    "focus-trap-react": "^11.0.3",
    "lucide-react": "^0.503.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.2.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudoperators/juno-ui-components": "^4.0.0",
    "@cobaltcore-dev/aurora-config": "workspace:*",
    "@fastify/vite": "^8.1.3",
    "@lingui/cli": "^5.3.2",
    "@lingui/swc-plugin": "^5.5.2",
    "@lingui/vite-plugin": "^5.3.2",
    "@tanstack/react-router-devtools": "^1.115.2",
    "@tanstack/router-plugin": "^1.115.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-lingui": "^0.10.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^6.2.7",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
