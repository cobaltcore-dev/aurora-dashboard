{
  "name": "@cobaltcore-dev/aurora-portal",
  "private": true,
  "version": "1.0.0",
  "main": "dist/client/index.js",
  "scripts": {
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --mode=production",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "check-i18n": "lingui extract --clean && lingui compile --typescript --verbose",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "clean:cache": "rm -rf .turbo",
    "compile": "lingui compile --typescript --verbose",
    "dev": "tsx watch src/server/server --tsconfig tsconfig.server.json",
    "extract": "lingui extract --clean",
    "lint": "eslint",
    "preview": "vite preview",
    "prod": "NODE_ENV=production node dist/server/server.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cobaltcore-dev/policy-engine": "workspace:*",
    "@cobaltcore-dev/signal-openstack": "workspace:*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^8.3.0",
    "@headlessui/react": "^2.2.9",
    "@lingui/core": "^5.5.2",
    "@lingui/react": "^5.5.2",
    "@material-design-icons/svg": "^0.14.15",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.36",
    "@testing-library/user-event": "^14.6.1",
    "@trpc/client": "11.0.0-rc.730",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "11.0.0-rc.730",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^16.6.1",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "focus-trap-react": "^11.0.4",
    "lucide-react": "^0.503.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^5.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudoperators/juno-ui-components": "^5.8.0",
    "@cobaltcore-dev/aurora-config": "workspace:*",
    "@fastify/vite": "^8.2.0",
    "@lingui/cli": "^5.5.2",
    "@lingui/swc-plugin": "^5.6.1",
    "@lingui/vite-plugin": "^5.5.2",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-router-devtools": "^1.133.36",
    "@tanstack/router-plugin": "^1.133.36",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-lingui": "^0.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
