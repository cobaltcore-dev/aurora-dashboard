{
  "name": "@cobaltcore-dev/aurora-portal",
  "private": true,
  "version": "1.0.0",
  "main": "dist/client/index.js",
  "scripts": {
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --mode=production",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "check-i18n": "lingui extract --clean && lingui compile --typescript --verbose",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "clean:cache": "rm -rf .turbo",
    "compile": "lingui compile --typescript --verbose",
    "dev": "tsx watch src/server/server --tsconfig tsconfig.server.json",
    "extract": "lingui extract --clean",
    "lint": "eslint",
    "preview": "vite preview",
    "prod": "NODE_ENV=production node dist/server/server.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cobaltcore-dev/policy-engine": "workspace:*",
    "@cobaltcore-dev/signal-openstack": "workspace:*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@headlessui/react": "^2.2.2",
    "@lingui/core": "^5.9.0",
    "@lingui/react": "^5.9.0",
    "@material-design-icons/svg": "^0.14.15",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.16",
    "@tanstack/react-virtual": "^3.13.18",
    "@testing-library/user-event": "^14.6.1",
    "@trpc/client": "11.8.0",
    "@trpc/react-query": "11.8.0",
    "@trpc/server": "11.8.0",
    "@types/d3": "^7.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.7.2",
    "fastify-plugin": "^5.1.0",
    "focus-trap-react": "^11.0.3",
    "lucide-react": "^0.503.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.1.0",
    "tailwind-merge": "^3.2.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudoperators/juno-ui-components": "^5.11.0",
    "@cobaltcore-dev/aurora-config": "workspace:*",
    "@fastify/vite": "^8.4.1",
    "@lingui/cli": "^5.9.0",
    "@lingui/swc-plugin": "^5.10.1",
    "@lingui/vite-plugin": "^5.9.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router-devtools": "^1.157.16",
    "@tanstack/router-plugin": "^1.157.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-lingui": "^0.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.7",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
